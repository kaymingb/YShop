
@{
    ViewBag.Title = "AgentTiXian";
    Layout = "~/Views/Shared/_LayoutAgent.cshtml";
}



<div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <h3 class="page-header">代理提现</h3>
    <div id="error-panel"></div>
    <div id="container" class="table-responsive">
        <form class="form-inline" id="_ModalForm">
            <table class="table table-striped table-hover table-bordered">
                <tr>
                    <td style="width:150px">
                        账号
                    </td>
                    <td colspan="5"><input readonly="readonly" class="form-control" type="text" value="@ViewBag.Account" /></td>
                </tr>
                <tr>
                    <td>
                        余额
                    </td>
                    <td colspan="5">
                        <input class="form-control" readonly="readonly"
                               placeholder="" type="text" value="@ViewBag.Money" />
                    </td>
                </tr>
                <tr>
                    <td>
                        提现金额
                    </td>
                    <td colspan="5">
                        <input class="form-control" name="Money" id="Money"
                               placeholder="" type="number" value="" />
                    </td>
                </tr>

                <tr>
                    <td>
                        提现到
                    </td>
                    <td colspan="5"><input readonly="readonly" class="form-control" type="text" value="@ViewBag.CalWay" /></td>
                </tr>

            </table>
            <div>
                <button type="button" onclick="dosub()" class="btn btn-primary">提交</button>
            </div>
        </form>
    </div>
</div>

<style>
    input {
        width: 400%;
        max-width: 100%;
    }

    input, select, textarea {
        max-width: 100%;
    }
</style>

<link href="~/plug/kindeditor/themes/default/default.css" rel="stylesheet" />
<link href="~/plug/kindeditor/plugins/code/prettify.css" rel="stylesheet" />
<script src="~/plug/kindeditor/kindeditor-min.js"></script>
<script src="~/plug/kindeditor/lang/zh_CN.js"></script>





<script type="text/javascript">
    function dosub() {
        //alert($("#Content").val());
        //return;
        var data = $("#_ModalForm").serialize();
        $.post("/Agent/AgentTiXianPost", data, function (res) {
            alert(res);
            location.reload();
        })
    }
</script>



