
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="full-screen" content="yes">
    <meta name="x5-fullscreen" content="true">
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=yes">
    <meta name="Keywords" content="游艇预定">
    <meta name="description" content="游艇预定" />
    <title>游艇预定</title>
    @{Html.RenderPartial("/views/PartV/HeadJC.cshtml");}
</head>
<body>
    <!--public header-->
    @{Html.RenderPartial("/views/PartV/Nav.cshtml");}


    <!--public aside bar-->
    @{Html.RenderPartial("/views/PartV/LeftNav.cshtml");}


    <!--banner -->
    <!--新版价格：20180428-->
    <section class="kzlistWrap kzACListWrap">
        <div class="container">
            <div class="kzBox kzallBox wow fadeInUp">
                <span class="kzACLabel">游艇预定:</span>
                <ul class="kzList kzallList">
                    <li><a href="#this">游艇预定</a></li>
                </ul>
            </div>
            <div class="kzactivitiesList wow fadeInLeft clearfix">
                @{
                    List<Yax.Model.Chw_Boat> list = ViewBag.list;
                    if (list != null && list.Count > 0)
                    {
                        for (int i = 0; i < list.Count; i++)
                        {
                            <div class="span3 col-sm-6">
                                <a href="/home/MHView?id=@list[i].ID" class="kzlistLi">
                                    <div class="kzimage">
                                        <img src="@(ViewBag.adminUrl+list[i].Cover)" />
                                    </div>
                                    <p class="kzbrief">
                                        @list[i].Name
                                    </p>
                                </a>
                            </div>
                        }
                    }
                }

         
            </div>
        </div>
    </section>






    @{Html.RenderPartial("/views/PartV/PhoneNav.cshtml");}
    <!--public footer-->
    @{Html.RenderPartial("/views/PartV/Footer.cshtml");}

    @{Html.RenderPartial("/views/PartV/FooterJC.cshtml");}


</body>
</html>
